# 后端环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 应用配置
APP_NAME=spot
APP_ENV=development
DEBUG=true

# 数据库配置
DATABASE_URL=mysql+asyncmy://root:your_password@127.0.0.1:3306/spot

# 安全配置
SECRET_KEY=your-secret-key-here-change-in-production

# CORS 配置
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# 日志配置
LOG_LEVEL=INFO
