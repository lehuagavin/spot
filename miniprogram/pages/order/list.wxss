/* 拼班记录样式 */
.container {
  padding: 20rpx;
}

.tabs {
  display: flex;
  background: white;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}

.tab {
  flex: 1;
  text-align: center;
  padding: 20rpx;
}

.tab.active {
  color: #1890ff;
  border-bottom: 2px solid #1890ff;
}

.order-list {
  background: white;
  border-radius: 10rpx;
  padding: 30rpx;
}

.empty {
  text-align: center;
  color: #999;
  padding: 100rpx 0;
}
